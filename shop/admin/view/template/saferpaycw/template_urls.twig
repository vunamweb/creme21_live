{{ header }}

<div id="content">
	<div class="breadcrumb">
		{%  for breadcrumb in breadcrumbs %}
			{{ breadcrumb['separator'] }}<a href="{{ breadcrumb['href'] | raw }}">{{ breadcrumb['text'] }}</a>
		{%  endfor %}
	</div>
	<div class="box">
		<div class="heading">
			<h1><img src="view/image/payment.png" alt="" />{{ heading_title }}</h1>
			<div class="buttons">
			</div>
		</div>
		<div class="content">
			{%  for url in templateUrls %}
				<a href="{{ url | raw }}" target="_blank">{{ url }}</a><br />
			{% endfor %}
		</div>
	</div>
</div>

<script type="text/javascript"><!--
$('#tabs a').tabs();
//--></script> 

{{ footer }}